{
    "request": {
        "method": "GET",
        "headers": {
            "Content-Type": {
                "equalTo": "application/vnd.pl.devoxx.aggregatr.v1+json"
            }
        },
        "url": "/ingredients",
        "bodyPatterns": [
            {
                "equalTo": "{\"items\":[\"WATER\",\"HOP\",\"YIEST\",\"MALT\"]}"
            }
        ]
    },
    "response": {
        "status": 200,
        "body": "{\"ingredients\":[{\"type\":\"MALT\",\"quantity\":100},{\"type\":\"WATER\",\"quantity\":200},{\"type\":\"HOP\",\"quantity\":300},{\"type\":\"YIEST\",\"quantity\":400}]}"
    }
}